        Favorito = int.Parse(Session["Favorito"].ToString());//Favorito
        Paqueteria = int.Parse(Session["Paqueteria"].ToString());//Paqueteria
        Porcentaje = int.Parse(Session["Porcentaje"].ToString());//Porcentaje
        Producto = int.Parse(Session["Producto"].ToString());//Producto
        Puesto = int.Parse(Session["Puesto"].ToString());//Puesto
        RutaBase = int.Parse(Session["RutaBase"].ToString());//RutaBase
        UnidadMedida = int.Parse(Session["UnidadMedida"].ToString());//UnidadMedida
        Almacen = int.Parse(Session["Almacen"].ToString());//Almacen

    }
    catch (Exception)
    {
        error = true;
    }
}

