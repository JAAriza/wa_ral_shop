        Carrito = int.Parse(Session["Carrito"].ToString());//CarritoCompras
        Categoria = int.Parse(Session["Categoria"].ToString());//Categoria
        Colaborador = int.Parse(Session["Colaborador"].ToString());//Colaborador
        Cuenta = int.Parse(Session["Cuenta"].ToString());//Cuenta
