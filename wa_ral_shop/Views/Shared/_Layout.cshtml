    int Colaborador = 0;
    int Cuenta = 0;
    int Documento = 0;
    int Favorito = 0;
    int Paqueteria = 0;
    int Porcentaje = 0;
    int Producto = 0;
    int Puesto = 0;
    int RutaBase = 0;
    int UnidadMedida = 0;
    int Almacen = 0;

    try
    {
        CDireccion = int.Parse(Session["CDireccion"].ToString());//ClienteDireccion
        Cliente = int.Parse(Session["Cliente"].ToString());//Cliente
        Compra = int.Parse(Session["Compra"].ToString());//Compra
        CPaqueteria = int.Parse(Session["CPaqueteria"].ToString());//ComentarioPaqueteria
        CProveedor = int.Parse(Session["CProveedor"].ToString());//ComentarioProveedor
